create table loan(loan_number int primary key,branch_name varchar(20),amount float,foreign key(branch_name)references branch(branch_name) on delete cascade);
insert into loan values(1,'xyz',200000.23);
insert into loan values(2,'abc',2060000.23);
insert into loan values(3,'efg',2500000.23);
insert into loan values(4,'hig',24500000.23);
insert into loan values(5,'klm',5000000.23);
show tables;
select * from loan;


